{
    "arguments": {
        "run": "fish",
        "bwrap": []
    },
    "mounts": {
        "rw": [
            [
                ".",
                "~/workspace"
            ]
        ],
        "ro": [
            [
                "~/.nix-profile",
                "~/.nix-profile"
            ],
            [
                "~/.config/fish/",
                "~/.config/fish/"
            ]
        ],
        "tmpfs": [
            "~"
        ]
    },
    "sandbox-cwd": "~/workspace",
    "hostname": "localhost"
}